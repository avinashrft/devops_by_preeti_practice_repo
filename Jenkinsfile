pipeline 
{
    stages{
        stage('compile')
        {
            steps{
               echo "Compile the code"
            }
        }
        stage('Unit Test')
        {
            steps{
               echo "Run the unitTest cases"
            }
        }
        stage('Package')
        {
            steps{
               echo "Packging the code"
            }
        }
    }
}