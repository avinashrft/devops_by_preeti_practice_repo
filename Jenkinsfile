pipeline 
{
    stages{
        stage('compile')
        {
            steps{
              sh echo "Compile the code"
            }
        }
        stage('Unit Test')
        {
            steps{
              sh echo "Run the unitTest cases"
            }
        }
        stage('Package')
        {
            steps{
              sh echo "Packging the code"
            }
        }
    }
}